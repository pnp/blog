            if ((0 < (v5.old || '').length) && $(el5).hasClass(v5.old)) {
                if (0 < (v5.new || '').length) $(el5).addClass(v5.new);
                $(el5).removeClass(v5.old);
            }
        });
    };
    const worker = (el) => {
        fixer(el);
        if (!$(el).hasClass('line-numbers')) $(el).addClass('line-numbers');
        if ($('> code', el).length < 1) $(el).wrapInner('<code></code>');
        $('> code', el).each((i, v) => {
