                (function($){
                    $(document).ready(function() {
                        var rootElement = $('.LabelsTaplet.custom-blog-article-label-widget .BlogLabelsTaplet .LabelsList');
                        var currentPageIndex = 0, totalLabelCount = 1, pageSize = 5;
                        var totoalPageIndex = Math.ceil(totalLabelCount/pageSize) - 1;

                        getLabels();

                        if(totalLabelCount > pageSize){
                            var prevButton = $('.lia-paging-page-previous a.lia-js-data-pageNum-2', rootElement);
                            var prevButtonDisable = $('.lia-paging-page-previous span.lia-js-data-pageNum-1', rootElement);
                            var nextButton = $('.lia-paging-page-next a.lia-js-data-pageNum-2', rootElement);
                            var nextButtonDisable = $('.lia-paging-page-next span.lia-js-data-pageNum-1', rootElement);

                            arrangelabelNavigationButtons();

                            prevButton.click(function(e) {
                                e.preventDefault();
                                if(currentPageIndex > 0) {
                                    currentPageIndex--;
                                    arrangelabelNavigationButtons();
                                    getLabels();
                                }
                            });

                            nextButton.click(function(e) {
                                e.preventDefault();
                                if(currentPageIndex < totoalPageIndex) {
                                    currentPageIndex ++;
                                    arrangelabelNavigationButtons();
                                    getLabels();
                                }
                            });

                            function arrangelabelNavigationButtons() {
                                if(currentPageIndex > 0){
                                    prevButton.show();
                                    prevButtonDisable.hide();
                                } else {
                                    prevButton.hide();
                                    prevButtonDisable.show();
                                }

                                if(currentPageIndex < totoalPageIndex) {
                                    nextButton.show();
                                    nextButtonDisable.hide();
                                } else{
                                    nextButton.hide();
                                    nextButtonDisable.show();
                                }
                            }
                        }

                        function getLabels(){
                            $.ajax({
                                type:'post',
                                url: '/plugins/custom/microsoft/o365/custom.blog-article-label-widget?tid=866169382852714757',
                                data: { messageId : '3168855', page: (currentPageIndex * pageSize), pageSize : pageSize},
                                dataType: 'html',
                                context: $(rootElement),
                                beforeSend: function(xhr, opts) {
                                    $('.labels-container', $(this)).empty();
                                    $('.labels-spinner', $(this)).removeClass("hidden");
                                },
                                error: function (jqXHR, exception) {
                                   $('.labels-spinner', $(this)).addClass("hidden");
                                   var errorMessage = '<div>An unknown error occurred, please try again. If you continue to get this error please send a screenshot and description of what led to the error occuring to <a href="mailto:techcommunity@microsoft.com" title="techcommunity@microsoft.com"> The Microsoft Tech Community</a>.</div>'
                                   $('.labels-container',$(this)).html(errorMessage);
                                },
                                success: function (data) {
                                    $('.labels-spinner', $(this)).addClass("hidden");
                                    $('.labels-container',$(this)).html(data);
                                }
                            });
                        }
                    });
                })(LITHIUM.jQuery);

