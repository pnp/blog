    ;(function($) {
        $(document).ready(function() {
            // add tabindex to the clear search span for keyboard accessibilty
            var liaCancelSearch = document.querySelector('span.lia-cancel-search');
            liaCancelSearch.setAttribute('tabindex', '0');
            
            var pcompleteVal = sessionStorage.getItem("pcomplete");
            var rewriteStaticId = function () {
            
            // rewrite static ids
            var duplicates = [
                // search widget
                '#form_UID',
                '#form_instance_key',
                // lingo dash links
                '#Lingotek_PG_Configuration',
                // profile page sidebar
                '#user-list'
            ];

            duplicates.forEach(function (eleItem, index) {
                var ele;
                var count = 0;

                while (document.querySelector(eleItem)) {
                    count++
                    ele = document.querySelector(eleItem);
                    ele.id = (ele.id + count);
                }
            });
          };

          if ($('#community-menu-wrapper-list').children().length == 6) {
              $('#search_slider').addClass('unauth');
          }


          $('.login-link.lia-authentication-link').attr('aria-label', 'Sign In');

          if ( readCookie("profileCompleteNotification") == "hide" || pcompleteVal == "hide") {
            $(".custom-profile-completeness-banner").slideUp(300);
          } else {
            $(".custom-profile-completeness-banner, .close-welcome-notification").slideDown(300);
          }

          rewriteStaticId();

          $("#closeWelcomeNotification").click(function() {
              createCookie("profileCompleteNotification", "hide", "30");
              $(".custom-profile-completeness-banner").slideUp(300);
          });

          $('.UserAvatarWrapper').click(function() {
              $('#community-menu').removeClass('show');
              $(".notifications-overlay-favorites").hide();
              $('.new-notification-wrapper').each(function() {
                  $(this).addClass('hidden');
                  $(this).parent().removeClass('show selected');
                  $(this).parent().find('.lia-notifications-messages-aggregate').removeClass('hidden');
              });
              $('.community-menu-item.community-menu-dropdown.show').removeClass("show");
              $('.dropdown.dropdown-events.dropdown-primary-nav.show').removeClass("show");
          });

            $(".UserAvatar a, .user-profile-card").keydown(function(e) {
                var profileCard = $(this).closest('.UserAvatarWrapper').find(".user-profile-card").get(0);
                if(e.keyCode === 40 || e.keyCode === 38) {
                    e.preventDefault();               
                    if(!($(profileCard).is(':visible'))) {
                        $(profileCard).show();
                        $(this).focus();
                    }
                    else {
                        $(profileCard).hide();
                        $(this).focus();
                    }
                }       
            });
            $( ".UserAvatarWrapper" ).on( "keydown", ".user-profile-card .message-button", function(e) {
                if(e.keyCode === 9) {
                    $(".user-profile-card").hide();
                }
            });

          $('#community-menu').on("click", ".community-menu-icon", function () {
              //$('.custom-shortcuts-wrapper').toggleClass('hidden')
              $(".show").not('#community-menu').each(function () {
                  $(this).removeClass('show');
                  $('.custom-shortcuts-wrapper').removeClass('hidden')
              })

              $('.new-notification-wrapper').each(function() {
                  $(this).addClass('hidden');
                  $(this).parent().removeClass('show selected');
                  $(this).parent().find('.lia-notifications-messages-aggregate').removeClass('hidden');
              });

              if ($(this).parent().hasClass('show')) {
                  $(this).parent().removeClass('show');
              } else {
                  $(this).parent().addClass('show');
                  $(".notifications-overlay-favorites").show();
              }
          });

          $(".community-menu-item.community-menu-dropdown a.community-menu-item-allow-link").click(function(e){
              e.stopPropagation();
          });

          $(".community-menu-item.community-menu-dropdown").click(function (e) {
              var ele;

              if (e.target.tagName === 'A') {
                  return;
              } else {
                  e.preventDefault();
                  ele = $(this).find('.dropdown-primary-nav');

                  $(".show").not(ele).each(function () {
                      $(this).removeClass('show');
                  })

                  $('.new-notification-wrapper').each(function() {
                      $(this).addClass('hidden');
                      $(this).parent().find('.lia-notifications-messages-aggregate').removeClass('hidden');
                      $(this).parent().removeClass('show selected');
                  });

                  if ($(ele).hasClass('show')) {
                      $(ele).removeClass('show');
                      $(this).removeClass('show');
                  } else {
                      $(ele).addClass('show');
                      $(".notifications-overlay-favorites").show();
                      $(this).addClass('show');
                  }
              }
          });

        $('.notif-list, .notif-mail').on('keydown', function (e) {
            if (e.keyCode === 40 || e.keyCode === 38) {
                e.preventDefault();

                $(".show").each(function () {
                    $(this).removeClass('show selected');
                })

                $(".notifications-overlay-favorites").hide();

                if ($(this).find('.new-notification-wrapper').hasClass('hidden')) {
                    $('.new-notification-wrapper').each(function() {
