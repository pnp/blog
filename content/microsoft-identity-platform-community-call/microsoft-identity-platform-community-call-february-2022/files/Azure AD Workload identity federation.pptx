            var c = item.name;
            var domain = item.domain;
            var d = new Date();

            d.setMonth(-12);

            str = c.trim() + "=; expires=" + d.toUTCString() + "; path=/";
            document.cookie = str;

            str = c.trim() + "=; expires=" + d.toUTCString() + "; domain=" + domain + "; path=/";
            document.cookie = str;

