                                e.preventDefault();
                                if(currentPageIndex > 0) {
                                    currentPageIndex--;
                                    arrangelabelNavigationButtons();
                                    getLabels();
                                }
                            });

                            nextButton.click(function(e) {
                                e.preventDefault();
                                if(currentPageIndex < totoalPageIndex) {
                                    currentPageIndex ++;
                                    arrangelabelNavigationButtons();
                                    getLabels();
